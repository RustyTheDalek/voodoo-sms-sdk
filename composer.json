{
  "name": "rustythedalek/voodoo-sms-sdk",
  "description": "An SDK to interact with the Voodoo SMS API.",
  "type": "library",
  "license": "MIT",
  "keywords": ["gsd", "rusty", "dalek", "voodoo", "sms", "sdk"],
  "homepage": "https://github.com/RustyTheDalek/voodoo-sms-sdk",
  "authors": [
    {
      "name": "Ian Jones",
      "email": "ianalexanderjones@outlook.com",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "GoldSpecDigital\\VoodooSmsSdk\\": "src/",
      "GoldSpecDigital\\VoodooSmsSdk\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.2",
    "guzzlehttp/guzzle": "^6.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.3",
    "squizlabs/php_codesniffer": "^3.2",
    "codedungeon/phpunit-result-printer": "^0.4.4",
    "psy/psysh": "^0.11.21",
    "vlucas/phpdotenv": "^2.4",
    "slevomat/coding-standard": "~8.0"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
